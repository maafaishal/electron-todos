{
  "name": "todos-application",
  "productName": "Todos Application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "electron": "electron .",
    "build-mac": "electron-packager . TodosApplication --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds ",
    "build-win": "electron-packager . TodosApplication --overwrite --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "build-linux": "electron-packager . TodosApplication --overwrite --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "electron": "^9.3.1",
    "electron-packager": "^14.2.1"
  }
}
